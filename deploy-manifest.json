{
  "version": 1,
  "framework": {
    "name": "angular-ssr",
    "version": "20.1.3"
  },
  "imageSettings": {
    "sizes": [320, 640, 750, 828, 1080, 1200, 1920, 2048, 3840],
    "domains": ["buyallmemes.com", "cdn.buyallmemes.com"],
    "formats": ["image/webp", "image/avif"]
  },
  "build": {
    "commands": [
      "npm ci",
      "npm run generate-routes",
      "npm run build"
    ],
    "publish": ".amplify-hosting",
    "baseDirectory": "blog"
  },
  "compute": [
    {
      "name": "default",
      "runtime": "nodejs20.x",
      "entrypoint": "server.mjs"
    }
  ]
}